{
  "extends": "eslint-config-airbnb",
  "rules": {
    "linebreak-style": [0, "windows"],
    "semi": [0],
    "no-mixed-spaces-and-tabs": [0],
    "class-methods-use-this": [0],
    "max-len": [0],
    "no-param-reassign": [0],
    "no-undef":[0],
    "consistent-return": [0],
    "no-shadow":0,
    "no-tabs": [0],
    "indent": [0],
    "no-plusplus": [0]
  }
}